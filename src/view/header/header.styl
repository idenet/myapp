
.header 
  position: relative
  color: #fff
  background: rgba(7, 17, 27, 0.2)
  overflow: hidden
  .header-wrapper
    position: relative
    padding: 12px 6px 9px 12px
    .logo
      display: inline-block
      width: 70px
      height: 24px
      font-size: 20px
      text-align: center
      color: #12bdfd
      margin-left: 10px
    .github
      display: inline-block
      float: right
      font-size: 20px
      line-height: 25px
      margin-right: 20px
      color: #000
  .background
    position: absolute
    top: 0
    left: 0
    width: 100%
    height: 100%
    z-index: -1
    filter: blur(10px)
